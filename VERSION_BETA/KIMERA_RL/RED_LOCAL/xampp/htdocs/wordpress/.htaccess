
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /RED_LOCAL/xampp/htdocs/wordpress/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /RED_LOCAL/xampp/htdocs/wordpress/index.php [L]
</IfModule>

# END WordPress